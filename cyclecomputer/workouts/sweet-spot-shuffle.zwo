        <Warmup Duration="600" PowerLow="0.4" PowerHigh="0.65" Cadence="90"/>
<IntervalsT Cadence="95" CadenceResting="75" Repeat="3" OnDuration="240" OffDuration="30" OnPower="0.95" OffPower="0.7"/>
<SteadyState Duration="360" Power="0.8" Cadence="85"/>
<IntervalsT Cadence="95" CadenceResting="75" Repeat="3" OnDuration="240" OffDuration="30" OnPower="0.95" OffPower="0.7"/>
